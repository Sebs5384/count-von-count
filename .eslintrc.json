{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": "off",
        "no-console": "off",
        "no-use-before-define": "off",
        "import/extensions": "off",
        "no-restricted-syntax": "off",
        "guard-for-in": "off",
        "eqeqeq": "off"
    }
}
